{
  "extends": "@wcpos/tsconfig/base.json",
  "include": [
    "src"
  ],
  "compilerOptions": {
    "isolatedModules": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noFallthroughCasesInSwitch": true,
    "module": "esnext"
  }
}
